public class Test{
	public static void main(String[] args) {
		int num1 = 4;
		int num2 = 2;
		
		int result = add_two_numbers(num1, num2);
		System.out.println("result = " + result);
		
//		Another way, give vlaues instead variables
		System.out.println("result = " + add_two_numbers(5, 8));
	}
	public static int add_two_numbers(int x1, int x2) {
		return x1 + x2;
	}
}